{
  "expo": {
    "name": "ZolePointApp",
    "slug": "zole-point-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/icon/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.festus.zolepointapp",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.festus.zolepointapp",
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icon/favicon.ico"
    },
    "experiments": {
      "typedRoutes": true
    },
    "plugins": [
      "expo-sqlite",
      "expo-asset"
    ]
  }
}
